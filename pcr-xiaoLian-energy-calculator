            localStorage.clear()


            $list = $("#list").children()
            $list.each(function (i, n) {
                $img = $(n).children()
                if($.trim($img.attr("src")) != ''){
                    var src = $img.attr("src")
                    src = src.slice(10, src.length-5)
                    $.each(data, function (i, ele) {
                        $.each(ele, function (i, n) {
                            if(n.name == src && n.name != "咲恋"){
                                //console.log(n.name + "----------->" + n.stationNum)
                                var min = Math.abs(n.stationNum - 430)
                                localStorage.setItem(n.name, min.toString())
                            }
                        })
                    })
                }
            })
            min = localStorage.getItem(localStorage.key(0))
            // console.log("getResult方法中的min:"+min)
            compareNum()
            var name = tempVarSaveClass.name
            // console.log("getResult方法中的name值:" + name)
            showDiv(name)

        }

        function compareNum() {
            tempVarSaveClass.name = ""
            tempVarSaveClass.min = 0

            for(var i = 1; i < localStorage.length; i++){
                var key = localStorage.key(i)
                var stationNum = localStorage.getItem(key)
                stationNum = parseInt(stationNum)
                if(stationNum < min){
                    tempVarSaveClass.name = key
                    tempVarSaveClass.min = stationNum
                    min = stationNum
                }
            }
            // console.log("min="+min)
        }

        function showDiv(name) {
            $list = $("#list").children()
            $list.each(function (i, n) {
                $img = $(n).children()
                if($.trim($img.attr("src")).indexOf(name) != -1){
                    $(n).css("border", "2px solid aqua")
                }
            })
        }

        var tempVarSaveClass = {
            name : "",
            min : 0 ,
            showTemp : function () {
                console.log("===============================")
                console.log("name:" + tempVarSaveClass.name)
                console.log("min:" + tempVarSaveClass.min)
                console.log("===============================")
            }
        }



    </script>
