    <script type="text/javascript">

        var localStorage = window.localStorage
        var min = 0

        $(function () {
            //为每一个角色绑定了单击事件
            f()
            //点击div清除里面的内容
            div()
        })

        function f() {
            $.each($("img[name=img]"), function (i, n) {
                $(n).on("click", function () {
                    // alert($(n).attr("src"))
                    //先获取图片的src属性
                    var src = $(n).attr("src");
                    //组装成为一个html
                    var htmlSrc = '<img name="img" src="'+ src +'" style="position: absolute;width: 75px;height: 75px">';
                    //遍历所有的id为list的div下的所有div
                    $list = $("#list").children()
                    // console.log($list)
                    $list.each(function (i, n) {
                        //获取子div的id
                        var id = $(n).attr("id")
                        //检查div中是否有内容
                        var html = $.trim($("#"+id).html())
                        // console.log(html)
                        if(html.indexOf(src) != -1){
                            alert("不能选择相同角色!")
                            return false;
                        }
                        //如果div中的内容为空，那么将图片填充到div中
                        if(html == ''){
                            //alert("id是"+id)
                            $("#"+id).html(htmlSrc)
                            return false;
                        }
                    })

                })
            })
        }

        function div() {
            $list = $("#list").children()
            // console.log($list.attr("src"))
            $list.each(function (i, n) {
                $(n).on("click", function () {
                    $(n).html("")
                    $(n).css("border", "2px solid")
                })
            })
        }

        function reset() {
            $list = $("#list").children()
            // console.log($list.attr("src"))
            $list.each(function (i, n) {
                $(n).html("")
                $(n).css("border", "2px solid")
            })
        }

